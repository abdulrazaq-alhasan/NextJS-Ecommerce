{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "required": true
    },
    "username": {
      "type": "string"
    },
    "amount": {
      "type": "decimal"
    },
    "products": {
      "type": "json"
    },
    "carts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::cart.cart",
      "mappedBy": "orders"
    }
  }
}
